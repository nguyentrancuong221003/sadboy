CREATE TABLE `doancuacuong`.user_feedback (`STT` INT(10) NULL , `id_user_feed` VARCHAR(20) NULL , `content` VARCHAR(2000) NULL , `imgs` VARCHAR(500) NULL , 
`date` VARCHAR(30) NULL , `number_like` VARCHAR(10) NULL , `number_dislike` VARCHAR(10) NULL , `id_user` VARCHAR(20) NULL , 
PRIMARY KEY (`id_user_feed`)) ENGINE = InnoDB;
ALTER TABLE `doancuacuong`.`user_feedback`
ADD `id_comment` VARCHAR(20) NULL AFTER `id_user`;

